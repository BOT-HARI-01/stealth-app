{
  "name": "stealth-window",
  "version": "1.0.0",
  "description": "A framework for building stealth applications",
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "node-builder": "cd native && node-gyp configure build",
    "build": "electron-builder",
    "rebuild": "electron-rebuild -f -w ."
  },
  "keywords": [],
  "author": "uncb",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@google/genai": "^1.3.0",
    "cohere-ai": "^7.17.1",
    "dotenv": "^17.2.0",
    "google": "^2.1.0",
    "node-window-manager": "^2.2.4",
    "tesseract.js": "^6.0.1",
    "util": "^0.12.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "electron": "^35.7.2",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "install": "^0.13.0",
    "nan": "^2.22.2",
    "node-addon-api": "^8.3.1",
    "node-gyp": "^11.1.0"
  },
  "build": {
    "extraResources": [
      {
        "from": "native/build/Release/",
        "to": "native/build/Release/"
      }
    ],
    "appId": "com.yourcompany.xyz",
    "productName": "stealth-window",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "src/**/*",
      "assets/icons/**/*",
      "package.json"
    ],
    "asar": true,
    "win": {
      "target": "nsis",
      "icon": "assets/icons/win/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icons/mac/icon.icns"
    }
  },
  "requestedExecutionLevel": "requireAdministrator"
}